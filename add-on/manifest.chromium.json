{
  "minimum_chrome_version": "111",
  "background": {
    "service_worker": "dist/bundles/backgroundPage.bundle.js"
  },
  "permissions": [
    "clipboardWrite",
    "contextMenus",
    "idle",
    "notifications",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": ["<all_urls>"],
  "incognito": "not_allowed"
}
