{
  "applications": {
    "gecko": {
      "id": "ipfs-companion-libdweb-build@ci.ipfs.team"
    }
  },
  "protocol_handlers": [],
  "experiment_apis": {
    "protocol": {
      "schema": "../node_modules/libdweb/src/protocol/protocol.json",
      "child": {
        "scopes": ["addon_child"],
        "paths": [["protocol"]],
        "script": "../node_modules/libdweb/src/protocol/client.js"
      },
      "parent": {
        "events": ["startup"],
        "scopes": ["addon_parent"],
        "paths": [["protocol"]],
        "script": "../node_modules/libdweb/src/protocol/host.js"
      }
    }
  }
}
